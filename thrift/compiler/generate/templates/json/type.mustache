<%={{ }}=%>
{
  {{#type:void?}}
  "type" : "void"
  {{/type:void?}}
  {{#type:string?}}
  "type" : "string"
  {{/type:string?}}
  {{#type:binary?}}
  "type" : "binary"
  {{/type:binary?}}
  {{#type:bool?}}
  "type" : "bool"
  {{/type:bool?}}
  {{#type:byte?}}
  "type" : "byte"
  {{/type:byte?}}
  {{#type:i16?}}
  "type" : "i16"
  {{/type:i16?}}
  {{#type:i32?}}
  "type" : "i32"
  {{/type:i32?}}
  {{#type:i64?}}
  "type" : "i64"
  {{/type:i64?}}
  {{#type:double?}}
  "type" : "double"
  {{/type:double?}}
  {{#type:float?}}
  "type" : "float"
  {{/type:float?}}
  {{#type:struct?}}
  "type" : "struct",
  "name" : "{{type:name}}"
  {{/type:struct?}}
  {{#type:enum?}}
  "type" : "enum",
  "name" : "{{type:name}}"
  {{/type:enum?}}
  {{#type:service?}}
  "type" : "service",
  "name" : "{{type:name}}"
  {{/type:service?}}
  {{#type:list?}}
  "type" : "list",
  {{#type:listElemType}}
  "inner_type" : {{> type}}
  {{/type:listElemType}}
  {{/type:list?}}
  {{#type:set?}}
  "type" : "set",
  {{#type:setElemType}}
  "inner_type" : {{> type}}
  {{/type:setElemType}}
  {{/type:set?}}
  {{#type:map?}}
  "type" : "map",
  {{#type:keyType}}
  "key_type" : {{> type}},
  {{/type:keyType}}
  {{#type:valueType}}
  "value_type" : {{> type}}
  {{/type:valueType}}
  {{/type:map?}}
}
